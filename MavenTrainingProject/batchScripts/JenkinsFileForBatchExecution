node {  

	stage 'Checkout'
	    git credentialsId: 'github-credentials',
	        url: 'https://github.com/sthamim/MavenTrainingProject.git'
    stage 'Init' 
		bat 'set'
	    bat '''
			echo %workspace%
			cmd.exe /c %workspace%/MavenTrainingProject/batchScripts/myBatchFile.bat
	        EXIT /B 0
	    '''
	    
    stage 'Deploy' 
   		 bat '''Nothing to Deploy'''
   		 

}